<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/introFragment">

    <fragment
        android:id="@+id/introFragment"
        android:name="com.example.membersgramtest.ui.fragment.IntroFragment"
        android:label="IntroFragment" >
        <action
            android:id="@+id/action_introFragment_to_privicyFragment"
            app:destination="@id/privicyFragment" />
    </fragment>
    <fragment
        android:id="@+id/privicyFragment"
        android:name="com.example.membersgramtest.ui.fragment.privicyFragment"
        android:label="privicyFragment" >
        <action
            android:id="@+id/action_privicyFragment_to_joinBotFragment"
            app:destination="@id/joinBotFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinBotFragment"
        android:name="com.example.membersgramtest.ui.fragment.JoinBotFragment"
        android:label="JoinBotFragment" >

        <action
            android:id="@+id/action_joinBotFragment_to_verifyFragment6"
            app:destination="@id/verifyFragment6" />
    </fragment>
    <fragment
        android:id="@+id/verifyFragment6"
        android:name="com.example.membersgramtest.ui.fragment.VerifyFragment"
        android:label="VerifyFragment" >
        <action
            android:id="@+id/action_verifyFragment6_to_sendSmsFragment"
            app:destination="@id/sendSmsFragment" />
    </fragment>
    <fragment
        android:id="@+id/sendSmsFragment"
        android:name="com.example.membersgramtest.ui.fragment.SendSmsFragment"
        android:label="SendSmsFragment" />


</navigation>